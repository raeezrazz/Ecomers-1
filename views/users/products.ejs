<script>
    function addToWishlist(ids){
        $.ajax({
        method: "POST",
        url: '/addToWishlist',
        data:JSON.stringify({id:ids}),
        contentType: 'application/json',
        success: function (response) {
            console.log("helo");
            console.log(response,"hello");
            const Toast = Swal.mixin({
                toast: true,
                position: 'top-end',
                showConfirmButton: false,
                timer: 2000,
            });

            if (response.added) {
                console.log("heloert");
                Toast.fire({
                    icon: 'success',
                    title: response.message
                });
            } else if (response.remove) {
                console.log("heloevrbg");
                Toast.fire({
                    icon: 'success',
                    title: response.message
                });
            } else {
                Toast.fire({
                    icon: 'error',
                    title: 'Please log in to perform this action.',
                    timer: 2000
                });
                setTimeout(function () {
                    window.location.href = '/logining';
                }, 1000);
            }
        },
        error: function (error) {
            console.error(error.responseText);
        }
    });
}

